<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/lib/paper.min.css">
    <title>Quiz Dashboard</title>
</head>

<body>
    <div class="paper container-lg margin-large ">
        <h2 class="margin-none">Quiz Monitor</h2>
    </div>
    <div class="paper container-lg margin-large">
        <div style="overflow-x: auto">
            <table>
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>State</th>
                        <% locals.questions.forEach(question => { %>
                        <th><%= question.id %></th>
                        <% }) %>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody>
                    <% locals.students.forEach((student, i) => { %>
                    <tr>
                        <td><%= ++i %></td>
                        <td style="white-space: nowrap;"><%= student.fullname %></td>
                        <td>None</td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>

    </div>
</body>

</html>